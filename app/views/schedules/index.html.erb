<h>wats up</h>

<div class="container">
  <div class="row">
    <h3>Schedules</h3>
    <div class="column">

        <tbody>
          <% @schedules.each do |f| %>

              <p>
              <%= f.title %>
              <table class="table">
              <tr>
              <% f.assignment.each do |key, array| %>
                <% if key == "Error"%>
                  <%= "No available schedule" %>
                  <% next %>
                <% end %>
                <td><%= key.time %></td>
                <td><% array.each do |g| %>
                  <%= g.first_name %>
                <% end %></td>
              <% end %>
              </tr>
              </table>
              <p><%= link_to 'remove schedule', schedule_path(:id => f.id), method: :delete %> </p>
            </p>

          <% end %>
        </tbody>

    </div>
  </div>
</div>

<p><%= link_to 'Tabling schedule', new_schedule_path %> </p>
